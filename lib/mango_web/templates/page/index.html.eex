<section class="page-title">
  <h1>Seasonal Products</h1>
</section>

<section class="row">
  <%= for product <- @seasonal_products do %>
    <div class="product">
      <div class="product-name"><%= product.name %></div>
      <div>INR <span class="product-price"><%= product.price %></span></div>
    </div>
  <% end %>
</section>
